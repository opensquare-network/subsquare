{
  "compilerOptions": {
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "**/node_modules",
    ".next",
    "**/.next",
    "dist",
    "**/dist",
    "build",
    "**/build",
    "coverage",
    "**/coverage",
    "**/__tests__/**",
    "**/*.test.js",
    "**/*.test.jsx",
    "**/*.spec.js",
    "**/*.spec.jsx",
    "**/*.stories.js",
    "**/*.stories.jsx",
    "packages/*/assets/**/*.svg",
    "packages/*/assets/**/*.jpg",
    "packages/*/assets/**/*.png",
    ".yarn",
    ".pnp.*",
    ".vercel",
    ".chopsticks"
  ]
}
